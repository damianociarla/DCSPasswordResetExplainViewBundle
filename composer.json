{
  "name": "dcs/password-reset-explain-view-bundle",
  "type": "symfony-bundle",
  "description": "This bundle provides the visual display of password recovery services through view and form",
  "keywords": ["User","Password","Symfony","Reset","Twig"],
  "license": "MIT",
  "authors": [
    {
      "name": "Damiano Ciarla",
      "email": "damiano.ciarla@gmail.org"
    }
  ],
  "require": {
    "php": ">=5.3.9",
    "symfony/form": "~2.8|~3.0",
    "symfony/validator": "~2.3|~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "dcs/user-core-bundle": "~1.0@dev",
    "dcs/password-reset-core-bundle": "~1.0@dev",
    "symfony/framework-bundle": "~2.8|~3.0"
  },
  "autoload": {
    "psr-4": {
      "DCS\\PasswordReset\\Explain\\ViewBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DCS\\PasswordReset\\Explain\\ViewBundle\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}